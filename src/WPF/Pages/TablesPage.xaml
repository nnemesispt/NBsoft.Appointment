<UserControl x:Class="NBsoft.Appointment.WPF.Pages.TablesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NBsoft.Appointment.WPF.Pages"
      xmlns:mui="http://firstfloorsoftware.com/ModernUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300">

    <Grid Style="{StaticResource ContentRoot}">
        <StackPanel>
            <TextBlock Text="{DynamicResource app.tables.title}" Margin="2,10,2,5" Style="{StaticResource Heading2}" />
            <Label Name="BBCDash" Content="{DynamicResource app.tables.description}" Margin="2" />
            <StackPanel Name="PnlLinks" Margin="20,5">
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
